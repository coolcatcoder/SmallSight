                    <ui:Button text="Button" display-tooltip-when-elided="true" name="P1" />
                    <ui:Button text="Button" display-tooltip-when-elided="true" name="P2" />
                    <ui:Button text="Button" display-tooltip-when-elided="true" name="P3" />
                    <ui:Button text="Button" display-tooltip-when-elided="true" name="P4" />
                    <ui:Button text="Button" display-tooltip-when-elided="true" name="P5" />
                </ui:VisualElement>
                <ui:VisualElement name="Curses" style="flex-grow: 1; background-color: rgba(0, 0, 0, 0); width: auto; height: auto;">
                    <ui:Button text="Button" display-tooltip-when-elided="true" name="C1" />
                    <ui:Button text="Button" display-tooltip-when-elided="true" name="C2" />
                    <ui:Button text="Button" display-tooltip-when-elided="true" name="C3" />
                    <ui:Button text="Button" display-tooltip-when-elided="true" name="C4" />
                    <ui:Button text="Button" display-tooltip-when-elided="true" name="C5" />
                </ui:VisualElement>
            </ui:VisualElement>
            <ui:Button text="Continue\n(Total cost must be 0!)" display-tooltip-when-elided="true" name="PerksAndCursesContinue" style="position: relative; left: auto; top: auto; align-self: auto; bottom: auto;" />
        </ui:VisualElement>
        <ui:VisualElement name="GameOver" style="flex-grow: 1; background-color: rgba(0, 0, 0, 0); bottom: 50%; visibility: hidden;">
            <ui:Label tabindex="-1" text="GameOver" display-tooltip-when-elided="true" name="GameOverText" style="-unity-text-align: upper-center; color: rgb(159, 17, 17); font-size: 100%; left: auto; bottom: auto;" />
            <ui:Button text="Continue" display-tooltip-when-elided="true" name="GameOverContinue" style="width: 30%; left: 35%; position: relative; top: 30%;" />
            <ui:Button text="Main Menu" display-tooltip-when-elided="true" name="GoToMainMenu" style="width: 30%; left: 35%; position: relative; top: 30%;" />
        </ui:VisualElement>
        <ui:VisualElement name="MainMenu" style="flex-grow: 1; background-color: rgba(0, 0, 0, 0); align-items: center; justify-content: flex-start; align-self: auto; right: auto; bottom: 50%; visibility: visible;">
            <ui:Label tabindex="-1" text="SmallSight" display-tooltip-when-elided="true" style="font-size: 90%; color: rgb(57, 9, 149); -unity-text-outline-width: 2px; -unity-text-outline-color: rgb(233, 30, 196);" />
            <ui:Slider picking-mode="Ignore" label="Delay Until Hold Movement" value="0.25" high-value="2" focusable="true" show-input-field="true" name="HoldDelay" style="width: 50%;" />
