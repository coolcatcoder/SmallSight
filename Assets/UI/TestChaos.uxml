            <ui:Slider picking-mode="Ignore" label="Min Input Detected" value="1" high-value="1" focusable="true" show-input-field="true" name="MinInputDetected" style="width: 50%;" />
            <ui:SliderInt picking-mode="Ignore" label="Render Distance" value="5" high-value="100" show-input-field="true" name="RenderDistance" low-value="1" direction="Horizontal" style="width: 50%;" />
